{
  "preset": "google",
  "requireCurlyBraces": [
     "do",
     "try",
     "catch"
  ],
  "disallowSpacesInsideObjectBrackets": null,
  "disallowMultipleVarDecl": "exceptUndefined",
  "requireSpaceAfterLineComment": true,
  "maximumLineLength": {
     "value": 80,
     "allowRegex": true
  },
  "jsDoc": {
    "requireParamTypes": true
  },
  "excludeFiles": [
    "node_modules/**",
    "coverage/**",
    "test/fixtures/generated-*.js"
  ]
}
